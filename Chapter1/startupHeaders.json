[
       {
          "description":"模拟带headers的post请求",
          "request":{
                 "uri":"/postWithHeaders",
                 "method":"post",
                 "headers":{
                       "content-type":"application/json"
                   },
                 "json":{
                   "name":"zhangsan",
                   "age":"18"
                   }
             },
           "response":{
             "status": 200,
             "json": {
               "zhangsan": "success",
               "status": "1"
             }
             }
       }
   ]